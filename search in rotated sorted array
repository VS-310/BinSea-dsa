#include <bits/stdc++.h>
using namespace std;

int searchrs(vector <int> &v,int n,int x){
    int low=0,high=n-1;
    while(low<=high){
        int mid=low+(high-low)/2;
        if(v[mid]==x) return mid;
        
        if(v[low]<=v[mid]){
            if(v[low]<=x && x<=v[mid]) high=mid-1;
            else low=mid+1;
        }
        if(v[mid]<=v[high]){
            if(v[mid]<=x && x<=v[high]) low=mid+1;
            else high=mid-1;
        }
    }
    return -1;
}

int main() {
    vector <int> v={7,8,9,1,2,3,4,5,6};
    int n=v.size(); int x=1;
    cout<<searchrs(v,n,x);
    return 0;
}
